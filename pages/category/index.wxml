<scroll-view scroll-y class="scrollPage">
	<view class="cu-bar bg-white solid-bottom">
		<view class="action">
			<text class="cuIcon-title text-orange "></text> 日常
		</view>
	</view>
	<view class='nav-list'>
		<navigator hover-class='none' url="../note/{{item.page}}/{{item.page}}" class="nav-li bg-{{note_item_color[item.page]}}" wx:for="{{daily_list}}" wx:key="index">
			<view class="nav-title">{{note_item[[item.page]]}}</view>
			<text class='cuIcon-{{item.icon}}'></text>
			<!-- <image src="../../images/nav/{{item.image}}"></image> -->
      <text class='icon-{{item.icon}}'></text>
		</navigator>
	</view>
	<view class="cu-bar bg-white solid-bottom margin-top">
		<view class="action">
			<text class="cuIcon-title text-orange "></text> 成长
		</view>
	</view>
	<view class='nav-list'>
		<navigator hover-class='none' url="../note/{{item.page}}/{{item.page}}" class="nav-li bg-{{note_item_color[item.page]}}" wx:for="{{grow_list}}" wx:key="index">
			<view class="nav-title">{{note_item[[item.page]]}}</view>
			<!-- <image src="../../images/nav/{{item.image}}"></image> -->
      <text class='icon-{{item.icon}}'></text>
		</navigator>
	</view>
	<view class="place-holder"></view>
</scroll-view>